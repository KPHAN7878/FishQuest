      "bundleIdentifier": "com.wiltsig.myapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d012a586-9f66-44fd-a84e-fd5d3e3846bc"
      }
    }
  }
}
